
machine my_ubuntu_server {
	cpus: 1
	ram: 512Mb
	iso: "/opt/ubuntu-16.04.6-server-amd64.iso"
	disk main: {
		size: 4Gb
	}
}

[no_snapshots: true]
test test_parent {
	my_ubuntu_server {
		print "Hello parent"
	}
}

[no_snapshots: true]
test test_child1: test_parent {
	my_ubuntu_server {
		print "Hello child1"
	}
}

[no_snapshots: true]
test test_child2: test_parent {
	my_ubuntu_server {
		print "Hello child1"
	}
}

[no_snapshots: true]
test test_child3: test_child1 {
	my_ubuntu_server {
		print "Hello child3"
	}
}
