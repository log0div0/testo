

machine my_ubuntu_server {
	cpus: 1
	ram: 512Mb
	iso: "/opt/ubuntu-16.04.6-server-amd64.iso"
	disk main: {
		size: 4Gb
	}
}

test test_parent {
	my_ubuntu_server {
		abort "test_parent abort"
	}
}

test test_child1: test_parent {
	my_ubuntu_server {
		print "test_child1 hello"
	}
}

test test_child2: test_parent {
	my_ubuntu_server {
		print "test_child2 hello"
	}
}
