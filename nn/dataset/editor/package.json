{
  "name": "dataset-editor",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "babel src --out-dir dist",
    "watch": "babel src --out-dir dist --watch --skip-initial-build",
    "dev": "npm run build && run-p watch \"start {1}\" --"
  },
  "dependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "electron": "^9.2.0",
    "immer": "^7.0.8",
    "konva": "^7.0.6",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-konva": "^16.13.0-6",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "styled-jsx": "^3.3.0",
    "use-image": "^1.0.6",
    "uuid": "^8.3.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime",
      "styled-jsx/babel"
    ]
  }
}
