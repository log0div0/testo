network nat {
	mode: "nat"
}

network net1 {
	mode: "internal"
}

param login "my_machine_login"
param password "1111"
param host_name "my-machine"
param mac "52:54:00:00:00:11"

machine my_machine {
	cpus: 1
	ram: 512Mb
	iso: "../iso.iso"
	disk_size: 4Gb
	nic nat: {
		attached_to: "nat"
		adapter_type: "e1000"
	}

	nic LAN: {
		attached_to: "net1"
		mac: "${mac}"
		adapter_type: "e1000"
	}
}

test parent_test {
	my_machine print "This is parent test running"
}

test child_test: parent_test  {
	my_machine print "This is child test running"
}
